				<!-- 7% bonus to base planetary income -->
				<Percentage_Income_Modifier>7</Percentage_Income_Modifier>
				<!-- plus an absolute 0 credits to planetary income -->
				<Absolute_Income_Modifier>0</Absolute_Income_Modifier>
